{
  "name": "rakkasjs",
  "version": "0.6.3",
  "type": "module",
  "bin": {
    "rakkas": "./cli.js",
    "rakkasjs": "./cli.js"
  },
  "files": [
    "dist",
    "cli.js",
    "*.d.ts",
    "vite-license.md",
    "react-helmet-async-license.txt",
    "json-s-license.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/client.js",
      "default": "./dist/server.js"
    },
    "./node-adapter": "./dist/node-adapter.js",
    "./vite-plugin": "./dist/vite-plugin.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts"
      ]
    }
  },
  "description": "Bleeding-edge React framework powered by Vite",
  "license": "MIT",
  "repository": "https://github.com/rakkasjs/rakkasjs",
  "homepage": "https://rakkasjs.org",
  "keywords": [
    "react",
    "vite",
    "nextjs",
    "ssr"
  ],
  "peerDependencies": {
    "react": "18",
    "react-dom": "18",
    "vite": "3"
  },
  "devDependencies": {
    "@brillout/json-serializer": "^0.5.1",
    "@cyco130/eslint-config": "^2.1.2",
    "@types/babel__core": "^7.1.19",
    "@types/micromatch": "^4.0.2",
    "@types/prettier": "^2.7.1",
    "eslint": "^8.26.0",
    "prettier": "^2.7.1",
    "publint": "^0.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-helmet-async": "^1.3.0",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.2"
  },
  "dependencies": {
    "@babel/core": "^7.19.6",
    "@babel/types": "^7.20.0",
    "@hattip/adapter-node": "^0.0.20",
    "@hattip/bundler-cloudflare-workers": "^0.0.20",
    "@hattip/bundler-deno": "^0.0.20",
    "@hattip/bundler-netlify": "^0.0.20",
    "@hattip/bundler-vercel": "^0.0.20",
    "@hattip/compose": "^0.0.20",
    "@hattip/core": "^0.0.20",
    "@hattip/polyfills": "^0.0.20",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vavite/connect": "^1.5.0",
    "@vavite/expose-vite-dev-server": "1.5.0",
    "@vavite/multibuild": "^1.5.0",
    "@vitejs/plugin-react": "^2.2.0",
    "cac": "^6.7.14",
    "cheerio": "^1.0.0-rc.12",
    "devalue": "^4.2.0",
    "fast-glob": "^3.2.12",
    "magic-string": "^0.26.7",
    "micromatch": "^4.0.5",
    "picocolors": "^1.0.0",
    "sirv": "^2.0.2"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "pnpm test:typecheck && pnpm test:lint && pnpm test:unit && pnpm test:package",
    "test:unit": "vitest run --reporter=verbose",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:lint": "eslint . --max-warnings 0 --ignore-pattern dist",
    "test:package": "publint"
  }
}